import numpy as np
rows1 = int(input("Enter matrix1 rows: "))
cols1 = int(input("Enter the matrix1 columns: "))
matrix1 = []
print("Enter the elements row by row: ")
for i in range (rows1):
    row1=[]
    for j in range (cols1):
        row1.append(int(input()))
    matrix1.append(row1)
print("matrix 1:\n ")
for i in range(rows1):
    for j in range(cols1):
        print(matrix1[i][j], end=" ")
    print()

rows2 = int(input("Enter matrix2 rows: "))
cols2 = int(input("Enter the matrix2 columns: "))
matrix2 = []
print("Enter the elements row by row: ")
for i in range (rows2):
    row2=[]
    for j in range (cols2):
        row2.append(int(input()))
    matrix2.append(row2)
print("matrix 2:\n ")
for i in range(rows2):
    for j in range(cols2):
        print(matrix2[i][j], end=" ")
    print()
if cols1 != rows2:
        print("Incompatible matrix dimensions for dot product.")
else:
    print("Dot product:\n")
    dot_matrix=np.dot(matrix1,matrix2)
    print(dot_matrix)
