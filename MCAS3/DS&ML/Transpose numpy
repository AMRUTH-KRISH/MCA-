import numpy as np
rows = int(input("Enter the number of rows: "))
cols = int(input("Enter the number of columns: "))
matrix = []
print("Enter the elements row by row: ")
for i in range (rows):
    row=[]
    for j in range (cols):
        row.append(int(input()))
    matrix.append(row)
print("matrix 1:\n ")
for i in range(rows):
    for j in range(cols):
        print(matrix[i][j], end=" ")
    print()

print("Transpose:\n")
matrixa=np.array(matrix)
trans1=matrixa.T
print(trans1)
